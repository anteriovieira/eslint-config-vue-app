{
  "name": "eslint-config-vue-app",
  "version": "1.3.1",
  "description": "ESLint configuration used by vbuild",
  "repository": {
    "url": "egoist/eslint-config-vue-app",
    "type": "git"
  },
  "main": "index.js",
  "files": [
    "*.js"
  ],
  "scripts": {
    "test": "echo 'no tests!' && npm run lint",
    "lint": "xo",
    "example": "eslint example/*.*"
  },
  "author": "egoist <0x142857@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "xo": "^0.18.2"
  },
  "xo": {
    "extends": "rem",
    "ignores": [
      "example/**"
    ]
  },
  "dependencies": {
    "babel-eslint": "^7.2.3",
    "eslint-config-rem": "^3.1.0",
    "eslint-config-xo": "^0.18.1",
    "eslint-plugin-vue": "^2.0.1"
  },
  "peerDependencies": {
    "eslint": "^3.15.0"
  },
  "eslintConfig": {
    "extends": "./index.js"
  }
}
